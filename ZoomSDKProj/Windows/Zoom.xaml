<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="ZoomSDKProj.Windows.Zoom"
             Title="Zoom"
             Loaded="ZoomPage_Loaded">
    <StackLayout>
        <StackLayout HorizontalOptions="FillAndExpand" VerticalOptions="CenterAndExpand">
            <Entry Placeholder="MeetingId" Text="{Binding MeetingId}"/>
            <Entry Placeholder="Password" Text="{Binding MeetingPassword}"/>
            <Button Text="Join Meeting" Command="{Binding JoinMeetingCommand}"/>
        </StackLayout>
    </StackLayout>
</ContentPage>